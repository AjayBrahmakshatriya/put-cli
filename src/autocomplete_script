_put_cli()
{
	local cur
	local IFS=$'\n'
	pid=$$
	cur="${COMP_WORDS[COMP_CWORD]}"
	COMPREPLY=()
	if [ $COMP_CWORD -eq 1 ];
	then
		COMPREPLY=($(compgen -W "$(cat ~/.put-cli/autocomplete/commands.txt)" -- ${cur}))
	elif [ -f ~/.put-cli/autocomplete/$pid ];
	then
		COMPREPLY=($(compgen -W "$(cat ~/.put-cli/autocomplete/$pid)" -- ${cur}))
	fi
	return 0
}
complete -F _put_cli put
